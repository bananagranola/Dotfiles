
super + Escape
	/home/amytcheng/.scripts/exit.sh

alt + f
	termite -e vifm $WINDOWS/My\ Documents ~/Dropbox/School/

alt + r
	/home/amytcheng/.scripts/dmenu.sh

alt + t
	termite

alt + w
	chromium

XF86AudioRaiseVolume
	amixer set Master 10+

XF86AudioLowerVolume
	amixer set Master 10-

XF86AudioMute
	amixer set Master toggle


alt + b
	/home/amytcheng/.scripts/randombackground.sh

alt + c
	urxvtc -title mcabber -e mcabber

alt + e
	urxvtc -title mutt -e mutt

super + Escape
	/home/amytcheng/.scripts/exit.sh

alt + f
	urxvtc -title vifm -e vifm ~/ ~/Dropbox/School/

alt + m
	urxvtc -title mocp -e mocp

alt + n
	urxvtc -title newsbeuter -e newsbeuter

alt + p
	scrot '%Y-%m-%d_$wx$h.png' -e 'mv $f ~/Downloads'

alt + r
	/home/amytcheng/.scripts/dmenu.sh

alt + t
	st -o $HOME

alt + w
	dwb

control + F11
	/home/amytcheng/.scripts/music_control.sh previous

control + F10
	/home/amytcheng/.scripts/music_control.sh pause

control + F9
	/home/amytcheng/.scripts/music_control.sh next

control + F2
	amixer -q set Master 10%+

control + F1
	amixer -q set Master 10%-

control + Escape
	amixer -q set Master toggle

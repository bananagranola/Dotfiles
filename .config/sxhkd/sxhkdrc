
super + Escape
	/home/amytcheng/.scripts/exit.sh

alt + f
	st -t vifm -e vifm ~ ~/Dropbox/School/

alt + r
	/home/amytcheng/.scripts/dmenu.sh

alt + t
	st -e tmux

alt + w
	firefox

XF86AudioRaiseVolume
	amixer set Master 10+

XF86AudioLowerVolume
	amixer set Master 10-

XF86AudioMute
	amixer set Master toggle
